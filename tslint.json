{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "max-line-length":false,
      "linebreak-style": [
          true,
          "LF"
      ],
      "no-any": false,
      "quotemark": [
          true,
          "single",
          "jsx-double"
      ],
      "indent": [
          true,
          "spaces",
          2
      ],
      "one-variable-per-declaration": false,
      "no-console": false,
      "semicolon": false,
      "no-shadowed-variable": false,
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "interface-name": false,
      "member-ordering": [
          false
      ],
      "no-empty": [
          false
      ],
      "arrow-parens": false,
      "max-classes-per-file": false,
      "variable-name": false,
      "no-unused-expression": false,
      "class-name": false,
      "trailing-comma": [
          true,
          {
            "multiline": {
              "objects": "always",
              "arrays": "always",
              "functions": "never",
              "typeLiterals": "ignore"
            }
          }
        ]
  },
  "rulesDirectory": []
}